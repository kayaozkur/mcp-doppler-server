# Integration Test Configuration
# Copy this file to .env.test and fill in your test values

# Required: Doppler service token with read/write access to test project
DOPPLER_TEST_TOKEN=dp.st.your_test_token_here

# Optional: Test project and config names (defaults shown)
DOPPLER_TEST_PROJECT=mcp-test
DOPPLER_TEST_CONFIG=development

# Set to true to run integration tests
RUN_INTEGRATION_TESTS=false